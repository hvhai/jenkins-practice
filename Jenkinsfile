pipeline {
  agent any
  stages {
    stage("init") {
      steps {
        echo 'hello Jenkins'
      }
    }

    stage("build") {
      steps {
        echo 'build project'
      }
    }

    stage("test") {
      steps {
        echo 'test project'
      }
    }

    stage("deploy") {
      steps {
        echo 'deploy project'
      }
    }
  }

}